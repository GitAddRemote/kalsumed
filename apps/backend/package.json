{
  "name": "@kalsumed/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "migration:dryrun": "typeorm-ts-node-esm migration:generate -d src/data-source.cli.ts --dr --pretty",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/data-source.cli.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/data-source.cli.ts",
    "migration:gen:name": "NAME=${NAME:-${npm_config_name:-Migration}} typeorm-ts-node-esm migration:generate src/migrations/$NAME -d src/data-source.cli.ts --pretty",
    "mig:new": "NAME=${npm_config_name:-Migration} docker compose -f ../../docker-compose.dev.yml --env-file ../../apps/backend/.env.development run --rm -e NAME backend sh -lc 'pnpm exec typeorm-ts-node-esm migration:generate src/migrations/$NAME -d src/data-source.cli.ts --pretty'",
    "mig:apply": "docker compose -f ../../docker-compose.dev.yml --env-file ../../apps/backend/.env.development run --rm backend pnpm -F @kalsumed/backend run migration:run",
    "mig:revert": "docker compose -f ../../docker-compose.dev.yml --env-file ../../apps/backend/.env.development run --rm backend pnpm -F @kalsumed/backend run migration:revert",
    "mig:check": "docker compose -f ../../docker-compose.dev.yml --env-file ../../apps/backend/.env.development run --rm backend pnpm -F @kalsumed/backend run migration:dryrun",
    "seed:run": "DOTENV_CONFIG_PATH=.env.development ts-node -r dotenv/config -r tsconfig-paths/register src/cli/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect-redis": "^7.1.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "joi": "^18.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.25",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/connect-redis": "^0.0.23",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.19.10",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "passport-apple": "^2.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "8.10.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
